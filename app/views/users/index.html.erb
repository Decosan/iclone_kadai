<h1>Users index</h1>
<% if @users.any? %>
  <ul class="media-list">
    <% @users.each do |user| %>
  	<li class="media">
  		<h4 class="media-left" style="color: blue;">
  			User:
  		</h4>
  		<div class="media-body">
  			<h4 class="media-heading">Name: <%= user.name %></h4>
  			Registered: <%= user.created_at %>
  		</div>
  	</li>
    <% end %>
  </ul>
<%= paginate @users %>
<% end %>
